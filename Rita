        "terms = nmf_results[10][\"terms\"]\n",
        "H = nmf_results[10][\"model\"].components_  # shape: (K, n_terms)\n",
        "\n",
        "top_n = 20\n",
        "topic_labels_k10 = {}\n",
        "for t in range(K):\n",
        "    top_idx = np.argsort(H[t])[-top_n:][::-1]\n",
        "    topic_labels_k10[t] = \", \".join(terms[top_idx])\n",
        "\n",
        "pd.DataFrame({\n",
        "    \"topic\": list(topic_labels_k10.keys()),\n",
